{
  "branches": [
    { "name": "+([0-9])?(.{+([0-9]),x}).x", "range": "1.x.x", "channel": "v1" },
    "main",
    { "name": "beta", "prerelease": true },
    { "name": "alpha", "prerelease": true }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogTitle": "# Changelog\n\nAll notable changes to this project will be documented in this file. See [Conventional Commits](https://conventionalcommits.org) for commit guidelines.\n\n"
      }
    ],
    [
      "@semantic-release/npm",
      {
        "npmPublish": false
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "bin/index.js",
          "CHANGELOG.md",
          "README.md",
          "package.json",
          "yarn.lock"
        ],
        "message": "chore: release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [{
          "path": "bin"
        }]
      }
    ]
  ]
}
